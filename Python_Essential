#SumInRange

def Sum(x,y):
    sum = 0
    for i in range(x,y):
        sum += i
    return sum
c = Sum(1,101)
print(c)


#SumOfAnyNumbers

def add(*p):
    total = 0
    for i in p:
        total += i
    return total

d = add(1,2,3,4,5)
print(d)


#Sum

def sum(f,x,y):
    total = 0
    for i in range(x,y):
        total += f(i)
    return total
sum(lambda a:a,3,6)
12
sum(lambda a:a*2,3,6)
24
sum(lambda a:a*a,3,6)
50


